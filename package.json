{
  "name": "sf-website",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.17.0",
    "@ducanh2912/next-pwa": "^9.1.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@studio-freight/compono": "^0.0.20",
    "@studio-freight/hamo": "^0.6.18",
    "@studio-freight/react-lenis": "0.0.22",
    "@studio-freight/tempus": "0.0.38",
    "@theatre/core": "^0.6.1",
    "@theatre/studio": "^0.6.1",
    "blaze-slider": "1.9.3",
    "clsx": "^1.2.1",
    "events": "^3.3.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.12.2",
    "idb-keyval": "^6.2.1",
    "jsonminify": "^0.4.2",
    "next": "13.4.4",
    "next-sitemap": "^4.1.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-use": "^17.4.0",
    "react-use-measure": "^2.1.1",
    "sass": "^1.63.6",
    "throttle-debounce": "5.0.0",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@next/bundle-analyzer": "^13.4.9",
    "@size-limit/preset-app": "^8.2.6",
    "@svgr/webpack": "^8.0.1",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "size-limit": "^8.2.6",
    "stats.js": "^0.17.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "react-is": "18.2.0",
      "throttle-debounce": "5.0.0"
    }
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/**/*.js"
    }
  ]
}
